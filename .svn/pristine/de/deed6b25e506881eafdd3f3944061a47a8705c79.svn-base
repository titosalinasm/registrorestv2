package pe.gob.indecopi.exception;

import java.io.Serializable;

public class ClsException extends RuntimeException  implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -6414726715373776289L;

	public ClsException(String message) {
		super(message);
	}
	
	public ClsException(String message, Throwable cause) {
		super(message, cause);
	}

}
