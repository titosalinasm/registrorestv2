package pe.gob.indecopi.bean;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

import com.fasterxml.jackson.annotation.JsonFormat;

public class ClsTramitePagoBean  implements Serializable{

	private static final long serialVersionUID = 1L;
	private String vcCodArancel;
	private String 	vcNroOperacion;
	private String 	vcNroPasarela;
	private Integer nuExisteSIA;
	@JsonFormat(shape=JsonFormat.Shape.STRING, pattern="yyyy-MM-dd", timezone="PST")
	private Date 	dtFechaOperacion;
	private String vcCodBanco;
	private String vcTipoDocumento;
	private Integer nuIdTipoPago;
	private BigDecimal nuMonto;
	
	
	
	public String getVcCodArancel() {
		return vcCodArancel;
	}
	public void setVcCodArancel(String vcCodArancel) {
		this.vcCodArancel = vcCodArancel;
	}
	public ClsTramitePagoBean() {
		// TODO Auto-generated constructor stub
	}
	public String getVcNroOperacion() {
		return vcNroOperacion;
	}
	public void setVcNroOperacion(String vcNroOperacion) {
		this.vcNroOperacion = vcNroOperacion;
	}
	public String getVcNroPasarela() {
		return vcNroPasarela;
	}
	public void setVcNroPasarela(String vcNroPasarela) {
		this.vcNroPasarela = vcNroPasarela;
	}
	public Integer getNuExisteSIA() {
		return nuExisteSIA;
	}
	public void setNuExisteSIA(Integer nuExisteSIA) {
		this.nuExisteSIA = nuExisteSIA;
	}
	public Date getDtFechaOperacion() {
		return dtFechaOperacion;
	}
	public void setDtFechaOperacion(Date dtFechaOperacion) {
		this.dtFechaOperacion = dtFechaOperacion;
	}
	public String getVcCodBanco() {
		return vcCodBanco;
	}
	public void setVcCodBanco(String vcCodBanco) {
		this.vcCodBanco = vcCodBanco;
	}
	public String getVcTipoDocumento() {
		return vcTipoDocumento;
	}
	public void setVcTipoDocumento(String vcTipoDocumento) {
		this.vcTipoDocumento = vcTipoDocumento;
	}
	public Integer getNuIdTipoPago() {
		return nuIdTipoPago;
	}
	public void setNuIdTipoPago(Integer nuIdTipoPago) {
		this.nuIdTipoPago = nuIdTipoPago;
	}
	public BigDecimal getNuMonto() {
		return nuMonto;
	}
	public void setNuMonto(BigDecimal nuMonto) {
		this.nuMonto = nuMonto;
	}


	
}
