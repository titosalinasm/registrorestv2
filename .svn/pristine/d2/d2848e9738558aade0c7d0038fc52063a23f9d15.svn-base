package pe.gob.indecopi.util;

import java.io.Serializable;

import org.springframework.boot.context.properties.ConfigurationProperties;
import org.springframework.stereotype.Component;

@Component
@ConfigurationProperties(prefix="path-file")
public class ClsConfigFilePath implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -5579995624678365202L;
	private String rutaUploadTmp;
	private String rutaUploadFinal;
	
	public ClsConfigFilePath() {
		// TODO Auto-generated constructor stub
	}

	public String getRutaUploadTmp() {
		return rutaUploadTmp;
	}

	public void setRutaUploadTmp(String rutaUploadTmp) {
		this.rutaUploadTmp = rutaUploadTmp;
	}

	public String getRutaUploadFinal() {
		return rutaUploadFinal;
	}

	public void setRutaUploadFinal(String rutaUploadFinal) {
		this.rutaUploadFinal = rutaUploadFinal;
	}


}
